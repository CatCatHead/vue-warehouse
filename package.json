{
  "name": "vue-warehouse",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "vite",
    "dev:api": "vite --mode development.api",
    "build": "vue-tsc --noEmit && vite build",
    "build:mock": "vue-tsc --noEmit && vite build --mode mock",
    "build:api": "vue-tsc --noEmit && vite build --mode production",
    "preview": "vite preview",
    "preview:mock": "vite preview --mode mock",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "axios": "^1.12.2",
    "echarts": "^6.0.0",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "^10.5.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vue": "^3.5.22",
    "vue-echarts": "^8.0.1",
    "vue-router": "^4.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@element-plus/theme-chalk": "^2.2.16",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "element-plus": "^2.11.4",
    "prettier": "3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vue-tsc": "^3.1.0"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "exactOptionalPropertyTypes": true
  }
}
